@page "/List/ProductList"

@using taedisoft.Blazor.DataModels;
@using taedisoft.BlazorServer.Services;


<h3>Product Details</h3>

@if (Projects != null)
{
    foreach (var item in Projects)
    {
        <div style="display:grid; grid-template-columns: 1fr 1fr 1fr 1fr 1fr">
            <div>@item.Id</div>
            <div>@item.Name</div>
            <div>@item.Price</div>
            <div>@item.Quantity</div>
        </div>
    }
}
 